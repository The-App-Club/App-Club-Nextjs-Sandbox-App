{
  "hosting": {
    "public": "out",
    "site": "react-nextjs-tailwindcss-hamburger-menu-app",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/about",
        "destination": "/about.html"
      },
      {
        "source": "/contact",
        "destination": "/contact.html"
      },
      {
        "source": "/price",
        "destination": "/price.html"
      }
    ]
  }
}
